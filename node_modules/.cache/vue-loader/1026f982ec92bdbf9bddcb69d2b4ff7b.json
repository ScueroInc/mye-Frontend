;\r\n      this.dialog = true;\r\n    },\r\n\r\n  \r\n\r\n    close() {\r\n      this.dialog = false;\r\n    },\r\n    limpiar() {\r\n      this.id = \"\";\r\n      this.nombres = \"\";\r\n      this.apellidos = \"\";\r\n      this.dni = \"\";\r\n      this.direccion = \"\";\r\n      this.telefono = \"\";\r\n    },\r\n    guardar() {\r\n      if (this.editedIndex > -1) {\r\n        //Código para editar\r\n\r\n        let me = this;\r\n        axios \r\n          .put(\"api/paciente\", {\r\n            id: me.id,\r\n            nombres: me.nombres,\r\n            apellidos: me.apellidos,\r\n            dni: me.dni,\r\n            direccion: me.direccion,\r\n            telefono: me.telefono\r\n          })\r\n          .then(function(response) {\r\n            me.close();\r\n            me.listar();\r\n            me.limpiar();\r\n          })\r\n          .catch(function(error) {\r\n            console.log(error);\r\n          });\r\n      } else {\r\n        //Código para guardar\r\n        let me = this;\r\n        axios\r\n          .post(\"api/paciente\", {\r\n            nombres: me.nombres,\r\n            apellidos: me.apellidos,\r\n            dni: me.dni,\r\n            direccion: me.direccion,\r\n            telefono: me.telefono\r\n          })\r\n          .then(function(response) {\r\n            me.close();\r\n            me.listar();\r\n            me.limpiar();\r\n          })\r\n          .catch(function(error) {\r\n            console.log(error);\r\n          });\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n"]}]}