)\r\n          .catch(function(error) {\r\n            console.log(error);\r\n          });\r\n      } else {\r\n        //Código para guardar\r\n        let me = this;\r\n        axios\r\n          .post(\"api/medicalequipment\", {\r\n            \r\n            statedescription : me.statedescription,\r\n            brand : me.brand,\r\n            name : me.name\r\n          })\r\n          .then(function(response) {\r\n            me.close();\r\n            me.listar();\r\n            me.limpiar();\r\n          })\r\n          .catch(function(error) {\r\n            console.log(error);\r\n          });\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n"]}]}